import adivinhação
import forca
print("*******************")
print ("escolha seu jogo")
print("*******************")

print("(1) forca, (2) adivinhação")

jogo = int (input("qual jogo?"))

if(jogo == 1):
    print("jogando o jogo da forca")
    forca.jogar()

elif(jogo == 2):
    print("jogando advinhação")
    adivinhação.jogar()
